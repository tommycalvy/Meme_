<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Meme_</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta http-equiv="Cache-control" content="no-cache">
    <link rel="stylesheet" href="public/stylesheets/styles.css">
    <link rel="shortcut icon" href="public/images/favicon.ico" type="image/x-icon">
    <script src="public/libraries/jquery-3.4.1.min.js"></script>
    <script src="public/libraries/konva.min.js"></script>
    <script src="public/scripts/search.js"></script>
  </head>
  <body>
    <div class="wrap">
      <div class="header">
        <div class="logo">
          <h1>Meme_</h1>
        </div>
      </div>
      <div class="content">
        <div class="editor">
          <div class="image-effects">
            <h4>Image Effects</h4>
            <div class="image-effect-buttons">
              <button type="button">Move Up</button>
              <button type="button">Move Down</button>
            </div>
            <div class="reverse-search">
              <button type="button" id="rev-img-but">Reverse Image Search</button>
            </div>
          </div>
        </div>
        <div class="canvas" id="stage-parent">
          <div id="container"></div>
        </div>
        <div class="search">
          <div id="image-search"></div>
        </div>
      </div>
    </div>
  </body>
  <script src="public/scripts/canvas.js"></script>
</html>
